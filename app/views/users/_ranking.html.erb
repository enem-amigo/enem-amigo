<table class="ranking">
  <thead>
    <tr class="clickable-row" data-href=<%= ranking_path %> >
      <th><%= link_to image_tag("trophy.jpg"), ranking_path %> <span>TOP 10</span></th>
    </tr>
  </thead>
  <tbody>
  <% top10.each do |x| %>
    <tr>
      <td><%= link_to x.name, "/users/#{x.id}" %></td>
    </tr>
  <% end %>
  </tbody>
</table>